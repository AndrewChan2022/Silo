ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS SDX NETCDF PDB_DRV TAURUS HDF5_DRV EXODUS_DRV BROWSER MESA_BUILD MESA_CLEAN SPI_CPPFLAGS SPI MESHTV MESHTVX MPEG_SUPPORTED PARALLEL MPI_LIBS MPI_CC CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CXX CXXFLAGS ac_ct_CXX F77 FFLAGS ac_ct_F77 CPP EGREP SET_MAKE RANLIB ac_ct_RANLIB MAKEDEPEND AR OS_RULE SDX_LIBS X_LIBS MESHTVX_CPPFLAGS VDL_INC VDL_LIB GRAPHICS_OBJ JPEG_LIB JPEG_CFLAGS SILOPY SHLIB_CREATEPROG SHLIB_SOEXT SHLIB_LIBEXT tmpbin tmpinc tmplib LIBOBJS LTLIBOBJS'
