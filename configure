unsigned long ulongval () { return (long) (sizeof (off64_t)); }
#include <stdio.h>
#include <stdlib.h>
int
main ()
{

  FILE *f = fopen ("conftest.val", "w");
  if (! f)
    exit (1);
  if (((long) (sizeof (off64_t))) < 0)
    {
      long i = longval ();
      if (i != ((long) (sizeof (off64_t))))
	exit (1);
      fprintf (f, "%ld\n", i);
    }
  else
    {
      unsigned long i = ulongval ();
      if (i != ((long) (sizeof (off64_t))))
	exit (1);
      fprintf (f, "%lu\n", i);
