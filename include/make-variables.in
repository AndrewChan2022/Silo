##							-*- makefile -*-
## Variables
##
SHELL=/bin/sh
CC=@CC@
CFLAGS=@CFLAGS@
CXX=@CXX@
CXXFLAGS=@CXXFLAGS@
CPPFLAGS=-I. -I@tmpinc@ @CPPFLAGS@
F77=@F77@
## Meshtv's scripts treat CPPFLAGS as the include flags
F77FLAGS=@FFLAGS@ -I. -I@tmpinc@ @CPPFLAGS@
LIBS=@LIBS@
LDFLAGS=@LDFLAGS@
X_LIBS=@X_LIBS@
AR=@AR@
RANLIB=@RANLIB@
INSTALL=@tmpbin@/install-sh -c -m 755
INSTALL_DATA=@tmpbin@/install-sh -c -m 644
RM=rm -f
TGTDEP=$(LIB)(&)

##
## Temporary installation directories
##
tmplib=@tmplib@
tmpbin=@tmpbin@
tmpinc=@tmpinc@


##
## Permanent installation directories
##
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@
libdir=@libdir@
includedir=@includedir@
