# Modifications:
#

## Programs and options...
@SET_MAKE@
@VARIABLES@

CPPFLAGS=-I@tmpinc@ @CPPFLAGS@

LIBS=-lsilo @LIBS@
LIBDEP=$(tmplib)/libsilo.a

## Files...
SRC=rawconvert.c
OBJ=$(SRC:.c=.o)
TARGETS=rawconvert

DISTRIB=

## Standard targets...

all:
	@echo
	@if (fmt</dev/null)>/dev/null 2>&1;then fmt="fmt -70";else fmt=cat;fi;\
	   echo 'The rawconvert tool is not compiled by default.	 To compile it,'\
	  'run "make rawconvert" from the silo/tools/rawconvert directory.' |\
	   $$fmt | sed 's/^/NOTICE: /'
	@echo

rawconvert: rawconvert.o $(LIBDEP)
	$(CC) $(CFLAGS) -o $@ rawconvert.o $(LDFLAGS) $(LIBS)

clean:
	$(RM) $(OBJ) $(TARGETS)
	$(RM) .depend

##
## Other targets...
##

@TARGETS@
